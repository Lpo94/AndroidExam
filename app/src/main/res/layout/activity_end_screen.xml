<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="@drawable/baggrund"
tools:context="com.example.lp.androidexam.EndScreenActivity">


    <Button
        android:id="@+id/Return_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:background="@drawable/buttons"
        android:onClick="ReturnToMenu"
        android:text="Return to menu" />

    <EditText
        android:id="@+id/RaceResultsTilte"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Race Result:"
        android:layout_centerHorizontal="true" />

    <EditText
        android:id="@+id/place1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="38dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="1test"
        android:layout_above="@+id/Return_button"
        android:layout_alignLeft="@+id/RaceResultsTilte"
        android:layout_alignStart="@+id/RaceResultsTilte" />

    <EditText
        android:id="@+id/place2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="2test"
        android:layout_marginBottom="26dp"
        android:layout_above="@+id/place3"
        android:layout_alignLeft="@+id/place1"
        android:layout_alignStart="@+id/place1" />

    <EditText
        android:id="@+id/place3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="3test"
        android:layout_below="@+id/Return_button"
        android:layout_alignLeft="@+id/place4"
        android:layout_alignStart="@+id/place4" />

    <EditText
        android:id="@+id/place4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="4test"
        android:layout_below="@+id/place2"
        android:layout_alignLeft="@+id/place1"
        android:layout_alignStart="@+id/place1"
        android:layout_marginTop="47dp" />


</RelativeLayout>
